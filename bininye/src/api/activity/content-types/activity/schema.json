{
  "kind": "collectionType",
  "collectionName": "activities",
  "info": {
    "singularName": "activity",
    "pluralName": "activities",
    "displayName": "Activity",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "activityStatus": {
      "enum": [
        "upcoming",
        "past"
      ],
      "type": "enumeration"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::activity-category.activity-category"
    },
    "date": {
      "type": "date"
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false
    },
    "location": {
      "type": "string"
    },
    "objectives": {
      "component": "activity.activity-objective-item",
      "repeatable": true,
      "type": "component"
    },
    "participantsLabel": {
      "type": "string"
    },
    "slug": {
      "required": true,
      "targetField": "title",
      "type": "uid"
    },
    "tags": {
      "component": "shared.tag",
      "repeatable": true,
      "type": "component"
    },
    "title": {
      "required": true,
      "type": "string"
    },
    "domain": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::domain.domain"
    },
    "programItems": {
      "component": "activity.activity-program-item",
      "repeatable": true,
      "type": "component"
    }
  }
}